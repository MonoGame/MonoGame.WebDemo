{
    "output": "$(OutDir)/bridge/",
    "outputFormatting": "Minified",
    "console": {
        "enabled": false
    },
    "sourceMap": {
        "enabled": false
    },
    "reflection": {
        "disabled": false
    },
    "generateTypeScript": false,
    "cleanOutputFolderBeforeBuild": false,
    "report": {
        "enabled": false
    },
    "rules": {
        "anonymousType": "Plain",
        "arrayIndex": "Plain",
        "autoProperty": "Plain",
        "boxing": "Plain",
        "integer": "Managed",
        "lambda": "Plain"
    },
    "html": {
        "disabled": true
    }
}
